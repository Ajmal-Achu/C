#include <stdio.h>
#include <string.h>
char stack[50];
int top = -1;
void push(char x) { stack[++top] = x; }
char pop() { return stack[top--]; }
int prec(char x) {
    if (x == '+' || x == '-') return 1;
    if (x == '*' || x == '/') return 2;
    return 0;
}
int main() {
    char infix[50], postfix[50], ch;
    int i = 0, k = 0;
    printf("Enter infix expression: ");
    scanf("%s", infix);
    while (infix[i] != '\0') {
        ch = infix[i];
        if ((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z') || (ch >= '0' && ch <= '9')) {
            postfix[k++] = ch;
        } else if (ch == '(') {
            push(ch);
        } else if (ch == ')') {
            while (stack[top] != '(') postfix[k++] = pop();
            pop();
        } else {
            while (top != -1 && prec(stack[top]) >= prec(ch)) postfix[k++] = pop();
            push(ch);
        }
        i++;
    }
    while (top != -1) postfix[k++] = pop();
    postfix[k] = '\0';
    printf("Postfix: %s\n", postfix);
    return 0;
}
